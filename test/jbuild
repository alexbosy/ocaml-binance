(jbuild_version 1)

(executable
 ((name        depth)
  (public_name binance-depth)
  (package     binance_ws)
  (modules     (Depth))
  (preprocess (pps (ppx_jane ppxlib.runner)))
  (libraries (ws rest alcotest-async))))

(executables
 ((names        (test))
  (modules      (Test))
  (libraries (ws rest alcotest-async))))

(alias
 ((name    runtest-binance)
  (deps    (test.exe))
  (action  (run ${<}))))

(alias
 ((name    runtest)
  (deps    ((alias runtest-binance)))))
